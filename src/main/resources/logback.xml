<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<configuration>
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <Pattern>%d{yyyy-MM-dd HH:mm:ss.SSS} %-5level %logger{64} - %msg%n</Pattern>
    </encoder>
  </appender>
  <logger name="com.homearchivist" level="ALL"/>
  <root level="INFO">
    <appender-ref ref="STDOUT" />
  </root>
</configuration>